{
  "name": "simianbv/json-schema",
  "description": "A utility package for creating JSON resources based on the laravel models provided, comes with a model generator and migration",
  "type": "project",
  "keywords": [
    "laravel",
    "json-schema"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Simian",
      "email": "it@simian.nl"
    }
  ],
  "require": {
    "php": ">=7.3",
    "laravel/framework": "6.*|7.*|8.*",
    "symfony/yaml": "^5.0|^4.2",
    "ext-json": "*",
    "nyholm/psr7": "^1.2",
    "simianbv/introspect": "1.*|2.*|3.*"
  },
  "autoload": {
    "psr-4": {
      "Simianbv\\JsonSchema\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Simianbv\\JsonSchema\\JsonSchemaServiceProvider"
      ]
    }
  },
  "minimum-stability": "beta"
}
